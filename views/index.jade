extends layout1
block content
    section.page.container          
      .row
        .span8
              .box
                .box-header
                  i.icon-table                  
                  h5 Content
                .box-content
                  #calendar
                  
        .span8
          .box.pattern.pattern-sandstone
                .box-header
                  i.icon-folder-open
                  h5
                    | Self PMS Status
                .box-content.box-table
                  table#sample-table.table.table-hover.table-bordered.tablesorter
                    thead
                      tr
                        th Status 
                        th(style="color:green") Approved                       
                                         
          .box.pattern.pattern-sandstone
                .box-header
                  i.icon-folder-open
                  h5
                    | Subordinate PMS Status
                .box-content.box-table
                  table#sample-table.table.table-hover.table-bordered.tablesorter
                    thead
                      tr
                        th PMS Step
                        th Approved
                        th Pending
                    tbody
                      tr
                        td Setgoal
                        td 
                          div(data-toggle="modal" data-target="#GSCCModal" style="cursor:pointer;color:green") 10
                        td 
                          div(data-toggle="modal" data-target="#GSCCModal" style="cursor:pointer;color:red") 10

       #GSCCModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel', aria-hidden='true')
        .modal-dialog
          .modal-content
            .modal-header
              button.close(type='button', data-dismiss='modal', aria-hidden='true') ×  
              h4#myModalLabel.modal-title Subordinate List
            .modal-body
              | ...
            .modal-footer
              button.btn.btn-default(type='button', data-dismiss='modal') Close
              button.btn.btn-primary(type='button') Check
          
      //.row
        .span8
        .span8
          .box.pattern.pattern-sandstone
                .box-header
                  i.icon-table
                  h5
                    | Status
                .box-content.box-table
                  table#sample-table.table.table-hover.table-bordered.tablesorter
                    thead
                      tr
                        th PMS Step
                        th Approved
                        th Pending
                    tbody
                      tr
                        td Setgoal
                        td 
                          div(data-toggle="modal" data-target="#GSCCModal" style="cursor:pointer;color:green") 10
                        td 
                          div(data-toggle="modal" data-target="#GSCCModal" style="cursor:pointer;color:red") 10
               
             
                
          
          
    #spinner.spinner(style='display:none;')
      | Loading…
